             \PY{n}{paraBEM}\PY{o}{.}\PY{n}{PanelVector3}\PY{p}{(}\PY{l+m+mi}{3}\PY{p}{,} \PY{l+m+mi}{4}\PY{p}{,} \PY{l+m+mi}{1}\PY{p}{)}\PY{p}{,} 
             \PY{n}{paraBEM}\PY{o}{.}\PY{n}{PanelVector3}\PY{p}{(}\PY{l+m+mi}{0}\PY{p}{,} \PY{o}{\PYZhy{}}\PY{l+m+mi}{1}\PY{p}{,} \PY{l+m+mi}{0}\PY{p}{)}\PY{p}{]}
        \PY{n}{p} \PY{o}{=} \PY{n}{paraBEM}\PY{o}{.}\PY{n}{Panel3}\PY{p}{(}\PY{n}{l}\PY{p}{)}
        \PY{n}{p}\PY{o}{.}\PY{n}{area}\PY{p}{,} \PY{n}{p}\PY{o}{.}\PY{n}{n}\PY{p}{,} \PY{n}{p}\PY{o}{.}\PY{n}{l}\PY{p}{,} \PY{n}{p}\PY{o}{.}\PY{n}{m}\PY{p}{,} \PY{n}{p}\PY{o}{.}\PY{n}{center}
